{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../../",
  "modulePaths": ["<rootDir>"],
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "globalSetup": "<rootDir>/src/testing/setup.ts",
  "globalTeardown": "<rootDir>/src/testing/teardown.ts"
}
